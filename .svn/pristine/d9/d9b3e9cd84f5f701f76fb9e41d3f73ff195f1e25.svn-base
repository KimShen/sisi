<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<alias name="broadcast.presence.current" alias="broadcast.presence" />
	<bean id="broadcast.presence.current" class="com.sissi.broadcast.impl.CurrentPresenceQueue">
		<constructor-arg ref="addressing" />
		<constructor-arg ref="builder.presence" />
	</bean>
	<alias name="broadcast.protocol.current" alias="broadcast.protocol" />
	<bean id="broadcast.protocol.current" class="com.sissi.broadcast.impl.CurrentProtocolQueue">
		<constructor-arg ref="addressing" />
	</bean>
	<bean id="broadcast.protocol.fans" class="com.sissi.broadcast.impl.MyFansProtocolQueue">
		<constructor-arg ref="addressing" />
		<constructor-arg ref="builder.jid" />
		<constructor-arg ref="relation.roster" />
	</bean>

	<bean id="builder.presence" class="com.sissi.broadcast.impl.CurrentPresenceQueue$DefaultPresenceBuilder" />
</beans>
