<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util" xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="messageMatchProcessor" class="com.sissi.process.finder.MatchProcessor">
		<constructor-arg>
			<bean class="com.sissi.process.message.MessageMatcher" />
		</constructor-arg>
		<constructor-arg>
			<bean class="com.sissi.process.message.MessageProcessor">
				<constructor-arg ref="addressing" />
				<constructor-arg ref="relationContext" />
			</bean>
		</constructor-arg>
	</bean>

</beans>
