<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util" xsi:schemaLocation="http://www.springframework.org/schema/beans 
	   http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="streamCondition" class="com.sissi.pipeline.process.ChainedFinder$Condition">
		<constructor-arg>
			<bean class="com.sissi.pipeline.process.ClassMatcher">
				<constructor-arg value="com.sissi.protocol.stream.Stream" />
			</bean>
		</constructor-arg>
		<constructor-arg>
			<bean class="com.sissi.pipeline.process.stream.StreamProcessor" />
		</constructor-arg>
	</bean>

</beans>
