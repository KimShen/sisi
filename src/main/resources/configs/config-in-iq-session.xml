<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean id="session.pro.2fans" class="com.sissi.pipeline.in.iq.session.Session2FansProcessor" parent="global.pro.utils">
		<property name="protocolQueue" ref="broadcast.protocol.fans" />
	</bean>
	<bean id="session.pro.4fans" class="com.sissi.pipeline.in.iq.session.Session4FansProcessor" parent="global.pro.utils" />
	<bean id="session.pro.4selfs" class="com.sissi.pipeline.in.iq.session.Session4SelfsProcessor">
		<constructor-arg ref="storage.box" />
	</bean>
	<bean id="session.pro.chained" class="com.sissi.pipeline.in.ChainedProcessor">
		<constructor-arg>
			<util:list>
				<ref bean="iq.pro.result.next" />
				<ref bean="session.pro.2fans" />
				<ref bean="session.pro.4fans" />
				<ref bean="session.pro.4selfs" />
			</util:list>
		</constructor-arg>
	</bean>

	<bean id="session.condition" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg>
			<bean class="com.sissi.pipeline.in.ClassMatcher">
				<constructor-arg value="com.sissi.protocol.iq.session.Session" />
			</bean>
		</constructor-arg>
		<constructor-arg ref="session.pro.chained" />
	</bean>

</beans>
