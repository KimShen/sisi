<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean id="muc.pro.check.exists" class="com.sissi.pipeline.in.iq.muc.MucCheckExistsProcessor" parent="global.pro.proxy">
		<constructor-arg ref="ucenter.muc.config.builder" />
	</bean>
	<bean id="muc.pro.check.relation.4self" class="com.sissi.pipeline.in.iq.muc.MucCheckRelation4SelfProcessor" parent="global.pro.proxy">
		<property name="relationContext" ref="ucenter.muc" />
	</bean>
	<bean id="muc.pro.check.relation.4fans" class="com.sissi.pipeline.in.iq.muc.MucCheckRelation4FansProcessor" parent="global.pro.proxy">
		<constructor-arg ref="ucenter.muc" />
		<property name="relationContext" ref="ucenter.muc" />
	</bean>
	<bean id="muc.pro.kick" class="com.sissi.pipeline.in.iq.muc.MucKickProcessor" parent="global.pro.proxy">
		<constructor-arg ref="ucenter.muc" />
		<property name="relationContext" ref="ucenter.muc" />
	</bean>
	<bean id="muc.pro.kick.2all" class="com.sissi.pipeline.in.iq.muc.MucKick2AllProcessor" parent="global.pro.proxy">
		<constructor-arg ref="ucenter.muc" />
		<constructor-arg ref="ucenter.muc.judger.kick" />
		<constructor-arg ref="ucenter.muc.config.builder" />
		<property name="relationContext" ref="ucenter.muc" />
	</bean>
	<bean id="muc.pro.kick.chain" class="com.sissi.pipeline.in.ChainedProcessor">
		<constructor-arg>
			<util:list>
				<ref bean="muc.pro.kick.2all" />
				<ref bean="muc.pro.kick" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="muc.pro.fork" class="com.sissi.pipeline.in.ChainedProcessor">
		<constructor-arg>
			<util:list>
				<ref bean="muc.pro.check.exists" />
				<ref bean="muc.pro.check.relation.4self" />
				<ref bean="muc.pro.check.relation.4fans" />
				<bean class="com.sissi.pipeline.in.ForkProcessor">
					<constructor-arg ref="finder.muc" />
				</bean>
			</util:list>
		</constructor-arg>
	</bean>

	<bean id="finder.muc" class="com.sissi.pipeline.in.ChainedFinder">
		<constructor-arg>
			<util:list>
				<ref bean="muc.pro.condition.none" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="muc.pro.condition" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg>
			<bean class="com.sissi.pipeline.in.iq.muc.MucSetMatcher">
				<constructor-arg ref="global.jid" />
			</bean>
		</constructor-arg>
		<constructor-arg ref="muc.pro.fork" />
	</bean>
	<bean id="muc.pro.condition.none" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg>
			<bean class="com.sissi.pipeline.in.iq.muc.MucSetRoleMatcher">
				<constructor-arg value="NONE" />
			</bean>
		</constructor-arg>
		<constructor-arg ref="muc.pro.kick.chain" />
	</bean>
</beans>
