<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean id="search.pro.fields" class="com.sissi.pipeline.in.iq.search.SearchFieldsProcessor">
		<constructor-arg ref="ucenter.search.fields.form" />
	</bean>
	<bean id="search.pro.fields.chained" class="com.sissi.pipeline.in.ChainedProcessor">
		<constructor-arg>
			<util:list>
				<ref bean="search.pro.check" />
				<ref bean="search.pro.fields" />
			</util:list>
		</constructor-arg>
	</bean>

	<bean id="finder.search.fields" class="com.sissi.pipeline.in.ChainedFinder">
		<constructor-arg>
			<util:list>
				<ref bean="search.condition.fields" />
			</util:list>
		</constructor-arg>
	</bean>

	<bean id="search.condition.fields" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="search.condition.fields.matcher" />
		<constructor-arg ref="search.pro.fields.chained" />
	</bean>
	<bean id="search.condition.fields.matcher" class="com.sissi.pipeline.in.iq.IQActionMatcher">
		<constructor-arg value="com.sissi.protocol.iq.search.Search" />
		<constructor-arg value="GET" />
	</bean>
</beans>
