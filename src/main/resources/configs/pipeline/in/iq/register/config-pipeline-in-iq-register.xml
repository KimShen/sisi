<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean id="register.pro.fields.init" class="com.sissi.pipeline.in.iq.register.RegisterFieldsProcessor">
		<constructor-arg ref="uncenter.register.fields.form" />
	</bean>
	<bean id="register.pro.fields.fork" class="com.sissi.pipeline.in.ForkProcessor">
		<constructor-arg ref="finder.register.fields" />
	</bean>
	<bean id="register.pro.store" abstract="true">
		<constructor-arg ref="ucenter.register.user" />
		<constructor-arg ref="iq.pro.error.noneclear" />
	</bean>
	<bean id="register.pro.store.simple" class="com.sissi.pipeline.in.iq.register.RegisterStoreSimpleProcessor" parent="register.pro.store" />
	<bean id="register.pro.store.simple.chained" class="com.sissi.pipeline.in.ChainedProcessor">
		<constructor-arg>
			<util:list>
				<ref bean="register.pro.store.simple" />
				<ref bean="iq.pro.result.stop" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="register.pro.store.multiple" class="com.sissi.pipeline.in.iq.register.RegisterStoreMultiProcessor" parent="register.pro.store" />
	<bean id="register.pro.store.multiple.check" class="com.sissi.pipeline.in.iq.register.RegisterStoreCheckProcessor" />
	<bean id="register.pro.store.multiple.chained" class="com.sissi.pipeline.in.ChainedProcessor">
		<constructor-arg>
			<util:list>
				<ref bean="register.pro.store.multiple.check" />
				<ref bean="register.pro.store.multiple" />
				<ref bean="iq.pro.result.stop" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="register.pro.fork" class="com.sissi.pipeline.in.ForkProcessor">
		<constructor-arg ref="finder.register" />
	</bean>
	<bean id="register.pro.store.fork" class="com.sissi.pipeline.in.ForkProcessor">
		<constructor-arg ref="finder.register.store" />
	</bean>

	<bean id="finder.register" class="com.sissi.pipeline.in.ChainedFinder">
		<constructor-arg>
			<util:list>
				<ref bean="register.condition.fields" />
				<ref bean="register.condition.store" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="finder.register.store" class="com.sissi.pipeline.in.ChainedFinder">
		<constructor-arg>
			<util:list>
				<!-- MUC first -->
				<ref bean="register.condition.store.muc" />
				<ref bean="register.condition.store.simple" />
				<ref bean="register.condition.store.multiple" />
			</util:list>
		</constructor-arg>
	</bean>
	<bean id="finder.register.fields" class="com.sissi.pipeline.in.ChainedFinder">
		<constructor-arg>
			<util:list>
				<!-- MUC first -->
				<ref bean="register.condition.fields.muc" />
				<!-- AnyMatcher -->
				<ref bean="register.condition.fields.init" />
			</util:list>
		</constructor-arg>
	</bean>

	<alias name="register.condition" alias="condition.register"/>
	<bean id="register.condition" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="register.condition.matcher" />
		<constructor-arg ref="register.pro.fork" />
	</bean>
	<bean id="register.condition.matcher" class="com.sissi.pipeline.in.ClassMatcher">
		<constructor-arg value="com.sissi.protocol.iq.register.Register" />
	</bean>
	<bean id="register.condition.fields" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="register.condition.fields.matcher" />
		<constructor-arg ref="register.pro.fields.fork" />
	</bean>
	<bean id="register.condition.fields.matcher" class="com.sissi.pipeline.in.iq.IQActionMatcher">
		<constructor-arg value="com.sissi.protocol.iq.register.Register" />
		<constructor-arg value="GET" />
	</bean>
	<bean id="register.condition.fields.init" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg>
			<util:constant static-field="com.sissi.pipeline.in.AnyMatcher.INSTANCE" />
		</constructor-arg>
		<constructor-arg ref="register.pro.fields.init" />
	</bean>
	<bean id="register.condition.store" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="register.condition.store.matcher" />
		<constructor-arg ref="register.pro.store.fork" />
	</bean>
	<bean id="register.condition.store.matcher" class="com.sissi.pipeline.in.iq.IQActionMatcher">
		<constructor-arg value="com.sissi.protocol.iq.register.Register" />
		<constructor-arg value="SET" />
	</bean>
	<bean id="register.condition.store.simple" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="register.condition.store.simple.matcher" />
		<constructor-arg ref="register.pro.store.simple.chained" />
	</bean>
	<bean id="register.condition.store.simple.matcher" class="com.sissi.pipeline.in.iq.register.RegisterStoreMatcher">
		<constructor-arg value="false" />
	</bean>
	<bean id="register.condition.store.multiple" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="register.condition.store.multiple.matcher" />
		<constructor-arg ref="register.pro.store.multiple.chained" />
	</bean>
	<bean id="register.condition.store.multiple.matcher" class="com.sissi.pipeline.in.iq.register.RegisterStoreMatcher">
		<constructor-arg value="true" />
	</bean>

	<import resource="config-pipeline-in-iq-register-muc.xml" />
</beans>
