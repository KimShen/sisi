<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:util="http://www.springframework.org/schema/util"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
	http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/util
	http://www.springframework.org/schema/util/spring-util-3.0.xsd">

	<bean id="starttls.pro" class="com.sissi.pipeline.in.starttls.StarttlsProcessor" />

	<alias name="starttls.condition" alias="condition.starttls" />
	<bean id="starttls.condition" class="com.sissi.pipeline.in.PairCondition">
		<constructor-arg ref="starttls.condition.matcher" />
		<constructor-arg ref="starttls.pro" />
	</bean>
	<bean id="starttls.condition.matcher" class="com.sissi.pipeline.in.ClassMatcher">
		<constructor-arg value="com.sissi.protocol.starttls.Starttls" />
	</bean>
</beans>
